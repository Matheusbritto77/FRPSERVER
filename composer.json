{
    "name": "seu/nome-do-projeto",
    "description": "Projeto com Doctrine ORM",
    "type": "project",
    "require": {
        "doctrine/orm": "^2.10",
        "symfony/cache": "^7.2",
        "zircote/swagger-php": "^5.0",
        "swagger-api/swagger-ui": "^5.19",
        "phpauth/phpauth": "^1.5"
    },
    "scripts": {
        "doctrine": "vendor/bin/doctrine"
    },
    "autoload": {
        "psr-4": {
